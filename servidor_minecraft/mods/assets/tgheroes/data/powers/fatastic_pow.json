{
  "name": "Plasticity",
  "modifiers": {
    "fiskheroes:transformation|parash": {
      "key": "PARASHUTE",
      "transformation": {
        "toggleData": "tgheroes:dyn/parash",
        "timerData": "tgheroes:dyn/parash_timer",
        "time": 10
      }
    },
	"fiskheroes:transformation|ball": {
      "key": "BALL",
      "transformation": {
        "toggleData": "tgheroes:dyn/ball",
        "timerData": "tgheroes:dyn/ball_timer",
        "time": 5
      },
      "soundEvents": {
        "ENABLE": "tgheroes:fantast_ball"
      }
    },
	"fiskheroes:controlled_flight": {
      "speed": 0.04,
      "boostSpeed": 0.04,
      "canBoost": false,
      "collision": {
        "blocks": {
          "stopFlying": false,
          "takeDamage": false
        },
        "entities": {
          "stopFlying": false,
          "dealDamage": false
        }
      },
      "knockback": 0.0
    },
	"fiskheroes:energy_projection": {
      "damageProfile": {
        "damage": 5.0,
        "types": {
          "BLUNT": 1.0
        },
        "properties": {
          "COOK_ENTITY": true,
          "HEAT_TRANSFER": 80
        }
      },
      "range": 10.0,
      "radius": 0.1,
      "soundEvents": {
		"SHOOT": "tgheroes:fantast_aim"
      }
    },
	  "fiskheroes:gliding": {
	"gliding": {
        "speed": 0.5,
        "lift": 1.7,
        "gravity": 0.8,
        "stall": 0.25
      },
      "isPowered": false,
      "isControlled": false,
      "canJumpActivate": true,
      "boostSpeed": 0.1,
      "collision": {
        "blocks": {
          "stopFlying": true,
          "takeDamage": false
        },
        "entities": {
          "stopFlying": true,
          "dealDamage": false
        }
      },
      "knockback": 0.75,
      "soundEvents": {
        "ENABLE": ["fiskheroes:cape_open", "fiskheroes:gliding"],
        "DISABLE": "fiskheroes:cape_close",
        "IMPACT_BLOCK": "fiskheroes:flight_impact_block",
        "IMPACT_ENTITY": "fiskheroes:flight_impact_entity"
      }
    },
	  "fiskheroes:blade": {
      "soundEvents": {
        "ENABLE": "fiskheroes:utility_belt_switch",
        "DISABLE": "fiskheroes:utility_belt_switch"
      }
    },
    "fiskheroes:tentacles": {
      "tentacles": [
        [1.0, -0.5, 1.5],
        [-1.0, -0.5, 1.5]
      ],
      "climbing": {
        "speed": 0.1,
        "range": 5,
        "tolerance": 8,
        "minAnchorPoints": 2,
        "anchorPointZones": [
          ["TOP", "RIGHT", "FRONT"],
          ["TOP", "LEFT", "FRONT"]
        ]
      },
      "range": 8.0,
      "damageProfile": {
        "damage": 10.0,
        "types": {
          "BLUNT": 0.5
        },
        "properties": {
          "DAMAGE_DROPOFF": 0.6,
          "HIT_COOLDOWN": 0
        }
      },
      "tentacleStrike": {
        "chargeTime": 30,
        "damageProfile": {
          "damage": 20.0,
          "types": {
            "BLUNT": 1.0
          },
          "properties": {
            "HIT_COOLDOWN": 0
          }
        }
      },
      "canBreakGlass": true,
      "soundEvents": {
        "ENABLE": "tgheroes:hand_strech",
        "DISABLE": "tgheroes:hand_strech",
        "JAB_START": "tgheroes:hand_jab",
        "GRAB_START": "tgheroes:hand_grab",
        "STRIKE_START": "tgheroes:hand_strike",
        "STRIKE_CHARGED": "fiskheroes:utility_belt_switch"
      }
    },
	"fiskheroes:damage_weakness": {
      "damageType": "COLD"
    },
	"fiskheroes:projectile_immunity": {
    },
    "fiskheroes:damage_immunity": {
      "damageType": "BULLET"
    }
  }
}
