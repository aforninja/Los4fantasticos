{
  "name": "Apocalypse Powers",
  "modifiers": {
    "fiskheroes:transformation": {
      "transformation": {
        "toggleData": "fiskheroes:dyn/giant_mode",
        "timerData": "fiskheroes:dyn/giant_mode_timer",
        "time": 20
      }
    },
    "fiskheroes:charged_beam": {
      "damageProfile": {
        "damage": 12.0,
        "types": {
          "COSMIC": 1.0
        },
        "properties": {
          "COOK_ENTITY": true,
          "HEAT_TRANSFER": 20
        }
      },
      "chargeTime": 20,
      "duration": 20,
      "cooldownTime": 100,
      "range": 32.0,
      "radius": 0.2,
      "soundEvents": {
        "SHOOT": "tgheroes:apocalypse_beam",
        "BEAM_AMBIENT": "fiskheroes:antimatter_blast_loop"
      }
    },
	"fiskheroes:controlled_flight": {
      "speed": 0.1,
      "boostSpeed": 0.25,
      "canBoost": false,
      "canRoll": false,
      "collision": {
        "blocks": {
          "stopFlying": false,
          "takeDamage": false
        },
        "entities": {
          "stopFlying": false,
          "dealDamage": false
        }
      },
      "knockback": 0.4,
      "soundEvents": {
        "ENABLE": ["fiskheroes:flight_loop"],
        "BOOST": "fiskheroes:flight_boost"
      }
    },
    "fiskheroes:repulsor_blast": {
      "damageProfile": {
        "damage": 10.0,
        "types": {
          "ENERGY": 1.0
        },
        "properties": {
          "ADD_KNOCKBACK": 1.0,
          "HIT_COOLDOWN": 0.0
        }
      },
      "range": 48.0,
      "speed": 20.0,
      "spread": 0.0,
      "radius": 0.1,
      "cooldownTime": 30,
      "soundEvents": {
        "SHOOT": "tgheroes:apocalypse_blast"
      }
    },
    "fiskheroes:size_manipulation": {
      "isInstant": false,
      "minSize": 1.2,
      "maxSize": 14.0,
      "soundEvents": {
        "GROW": ["fiskheroes:anti_grow_roar", "fiskheroes:anti_grow", "fiskheroes:anti_grow_antimatter"],
        "GROW_STOP": "fiskheroes:anti_grow_stop",
        "SHRINK": "fiskheroes:anti_shrink"
      }
    },
    "fiskheroes:shield": {
      "shield": {
        "health": 100.0,
        "regeneration": 15.0,
        "recoveryDelay": 100,
        "cooldown": 150
      },
      "coverage": {
        "yawRadius": 180.0,
        "pitchRadius": 180.0,
        "yawOffset": 0.0,
        "pitchOffset": 0.0
      },
      "knockback": 0.0,
      "isToggle": false,
      "soundEvents": {
        "BLOCK_START": "fiskheroes:anti_forcefield",
        "DEFLECT": "fiskheroes:anti_forcefield_deflect"
      }
    },
    "fiskheroes:teleportation": {
      "canReachMoon": true,
      "range": 128.0,
      "soundEvents": {
        "TELEPORT": "fiskheroes:breach"
      }
    },
    "fiskheroes:telekinesis": {
      "telekinesis": {
        "crushMelons": true,
        "crushThrowables": true,
        "squeezeChickens": true,
        "explodeCreepers": true,
        "destroyInanimates": true
      },
      "canGrab": {
        "mobs": true,
        "items": true,
        "projectiles": true,
        "inanimates": true
      },
      "range": 32.0,
      "soundEvents": {
        "GRAB": ["fiskheroes:telekinesis_start", "fiskheroes:telekinesis_loop"]
      }
    },
    "fiskheroes:projectile_immunity": {
    },
    "fiskheroes:water_breathing": {
    },
    "fiskheroes:potion_immunity": {
      "potionEffects": ["minecraft:nausea", "minecraft:poison", "minecraft:wither"]
    },
    "fiskheroes:regeneration": {
      "factor": 6
    }
  },
  "hud": [
    {
      "type": "CHARGE",
      "texture": "tgheroes:textures/gui/bars/apocalypse.png",
      "data": "fiskheroes:beam_charge"
    }
  ]
}
