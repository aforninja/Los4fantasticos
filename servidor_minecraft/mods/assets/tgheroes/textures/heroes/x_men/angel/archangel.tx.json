{
  "texture": {
    "in": "tgheroes:x_men/angel/arch_angel",
    "transform": [
      {
        "operation": "XOR",
        "supply": "tgheroes:x_men/angel/xor_arch/arch_<TIMER>"
      }
    ],
    "out": "tgheroes:generated/angel_<TIMER>"
  },
  "getters": {
    "TIMER": {
      "key": "Math.ceil(16 * entity.getInterpolatedData('tgheroes:dyn/archangel_timer')) | 0",
      "values": {
        "min": 1,
        "max": 16
      }
    }
  },
  "isDynamic": true
}
